<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <Copyright>Copyright © Christian Woltering 2016-2022</Copyright>
    <AssemblyVersion>2.0.0.0</AssemblyVersion>
    <FileVersion>2.0.0.0</FileVersion>
    <PackageIcon></PackageIcon>
    <Authors>Christian Woltering</Authors>
    <ApplicationIcon>Resources\app.ico</ApplicationIcon>
    <OutDir>$(ProjectDir)..\bin\$(Configuration)</OutDir>
    <RootNamespace>fftbench</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="app.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="OxyPlot.WindowsForms" Version="2.1.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\fftbench.Common\fftbench.Common.csproj" />
  </ItemGroup>

</Project>
